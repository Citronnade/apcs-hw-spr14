

public class Node<E>{

    private E data;
    private Node next;

    public Node(E d){
	data = d;
	next = null;//not very necessary
    }

    public void setData(E d){
	data = d;
    }

    public String getData(){
	return data;
    }

    public void setNext(Node n){
	next = n;
    }

    public Node getNext(){
	return next;
    }

    public String toString(){
	return data;
    }

}


